/* vowels or consonants*/

import java.util.*;
public class Java_code{
    public static void main(String args[]) {
        Scanner sc =new Scanner(System.in);
        char chr=sc.next().charAt(0);
        chr=Character.toLowerCase(chr);
        if(chr=='a'|| chr=='e'||chr=='i'||chr=='o'||chr=='u')
        System.out.println("given chracter is vowel");
        else
            System.out.println("given char is cosonants");
        }
    }


/*check wheather character is uppercase or lowercase*/

import java.util.*;
public class Java_code{
    public static void main(String args[]) {
        Scanner sc =new Scanner(System.in);
        char chr=sc.next().charAt(0);
        if(chr>=97|| chr<=122)
            System.out.println("given character is in lowercase");
        else
            System.out.println("given char is in uppercase");
    }
}

/*input week number and print week days*/

import java.util.*;
public class Java_code{
    public static void main(String args[]) {
        Scanner sc =new Scanner(System.in);
        int week_num=sc.nextInt();
        if(week_num==1)
            System.out.println("Monday");
        else if(week_num==2)
            System.out.println("Tuesday");
        else if(week_num==3)
            System.out.println("Wednesday");
        else if(week_num==4)
            System.out.println("Thursday");
        else if(week_num==5)
            System.out.println("Friday");
        else if(week_num==6)
            System.out.println("Saturday");
        else if(week_num==7)
            System.out.println("Sunday");
        else
            System.out.println("input number between 1-7 only");
    }
}


/*input month number and print number of days in that month */

import java.util.*;
public class Java_code{
    public static void main(String args[]) {
        Scanner sc =new Scanner(System.in);
        int month_num=sc.nextInt();
        if(month_num==1|| month_num==3||month_num==5||month_num==7||month_num==8||month_num==10||month_num==12)
            System.out.println("the month contain 31 days");
        else if(month_num==4||month_num==6||month_num==9||month_num==11)
            System.out.println("The month contain 30 days ");
        else if(month_num==2)
            System.out.println("the month contain 28 or 29 days");
        else
            System.out.println("input number between 1-12 only");
    }
}



