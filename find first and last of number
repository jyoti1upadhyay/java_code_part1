import java.util.Scanner;
public class Java_code {
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        int num=sc.nextInt();
        int last=num%10;

        int sum=0;
        while(num>0){
        int rem=num%10;
        sum=sum*10+rem;
        num=num/10;}
        int first=sum%10;
        System.out.println(first);
        System.out.println(last);

}}
