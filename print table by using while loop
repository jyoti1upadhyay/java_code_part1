import java.util.Scanner;
public class Java_code {
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n= sc.nextInt();
        int i=1;
        while (i<=n){
            int mul=2*i;
            System.out.println(2 +"x"+i +"="+mul);
            i=i+1;
        }

    }
}
